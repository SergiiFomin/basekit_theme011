                        <article class="span4">

                            <div class="banner color2">
                                {{widget('content', 'ban-title3', {content: '<h2>Reservations</h2>', lines: 'all'})|raw}}
                                {{widget('content', 'ban-text3', {content: '<p>Lorem ipsum dolor sit amet conse ctetur adipisicing elit, sed do eiusmod tempor incididunt</p>', lines: 'all'})|raw}}
