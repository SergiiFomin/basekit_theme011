                <div class="row">

                    <article class="span12">

                        {{zone('main')|raw}}
                    
                        {{widget('space', 'space', {'height': '40'})|raw}}

                        {{widget('content', 'title1', {content: '<h3>Today is special offers</h3>', lines: 'all'})|raw}}

                        <div class="row offer">

                            <article class="span3">

                                {{widget('image', 'offer-image1', { 'src': asset('images/page1-img1.jpg'), 'imageWidth': '252px'})|raw}}
