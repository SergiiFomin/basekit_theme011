                                {{widget('content', 'offer-title3', {content: '<h5><a href="#">Dolor sit amet conse</a></h5>', lines: 'all'})|raw}}
                                {{widget('content', 'offer-text3', {content: 'Ut enim ad minim veniam, quis nostrud exercitation ullamco laboris nisi ut aliquip ex ea commodo consequat. Duis aute irure dolor in reprehenderit', lines: 'all'})|raw}}
