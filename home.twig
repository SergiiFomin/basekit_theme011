                                {{widget('space', 'space', {'height': '30'})|raw}}

                            </article>

                            <article class="span4">

                                <div class="banner color1">
                                    {{widget('image', 'banner1', { 'src': asset('images/banner1.jpg'), 'imageWidth': '153px'})|raw}}
                                    <div class="overflow">
                                        {{widget('content', 'ban-title1', {content: '<h2>Our</h2>', lines: 'all'})|raw}}
                                        {{widget('content', 'ban-subtitle1', {content: '<h3>HISTORY</h3>', lines: 'all'})|raw}}
                                        {{widget('content', 'ban-text1', {content: 'Lorem ipsum dolor sit amet conse ctetur adipisicing elit, sed do eiusmod tempor', lines: 'all'})|raw}}
                                    </div>
                                </div>
                                
                                {{widget('space', 'space', {'height': '14'})|raw}}

                                <div class="banner color2">
                                    {{widget('image', 'banner2', { 'src': asset('images/banner2.jpg'), 'imageWidth': '153px'})|raw}}
                                    <div class="overflow">
                                        {{widget('content', 'ban-title2', {content: '<h2>Our</h2>', lines: 'all'})|raw}}
                                        {{widget('content', 'ban-subtitle2', {content: '<h3>Events</h3>', lines: 'all'})|raw}}
                                        {{widget('content', 'ban-text2', {content: 'Lorem ipsum dolor sit amet conse ctetur adipisicing elit incididunt', lines: 'all'})|raw}}
                                    </div>
                                </div>
                
                                {{widget('space', 'space', {'height': '30'})|raw}}

                            </article>

                        </div>

                    </article>

                </div>

            </div>

        </div>

        <div class="maintop">

