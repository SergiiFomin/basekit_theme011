{% extends "layout.twig" %}

{% block content %}

<div class="bg-glow">
    
    <div class="global">

        <!--header-->
        <header>

            <div class="header">

                <div class="container">

                    <div class="row">

                        <article class="span12">

                            {{widget('space', 'space', {'height': '40'})|raw}}

                            <ul class="nav">
                                <li>{{widget('content', 'nav1', {content: '<a href="#">Register</a>', lines: 'all'})|raw}}</li>
                                <li>{{widget('content', 'nav2', {content: '<a href="#">Login</a>', lines: 'all'})|raw}}</li>
                                <li>{{widget('content', 'nav3', {content: '<a href="#">Site Map</a>', lines: 'all'})|raw}}</li>
                                <li>{{widget('content', 'nav4', {content: '<a href="#">Member Area</a>', lines: 'all'})|raw}}</li>
                                <li>{{widget('content', 'nav5', {content: '<a href="#">Search</a>', lines: 'all'})|raw}}</li>
                            </ul>

                            {{widget('space', 'space', {'height': '20'})|raw}}

                            {{widget('logo', 'name', {"defaultlogo": asset('images/logo.png')})|raw}}
                            {{widget('space', 'space', {'height': '30'})|raw}}

                            {{widget('navigation', 'navigation', {'align': 'left'})|raw}}
